  "version": "0.2.0",
  "configurations": [
    {
      "name": "flutter_application_1",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "$FLUTTER_WEB_PORT",
        "--web-hostname",
        "0.0.0.0",
        "--observatory-port",
        "$FLUTTER_DEBUG_PORT"
